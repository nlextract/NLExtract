    </complexType>
    <element name="Terrein" type="top10nl:TerreinType" substitutionGroup="top10nl:_Top10nlObject">
        <annotation>
            <documentation>Zichtbaar begrensd stuk grond, niet zijnde een van de andere opdelende ruimtelijke objecten
                (wegdeel of waterdeel)
            </documentation>
        </annotation>
    </element>
    <complexType name="TerreinType">
        <complexContent>
            <extension base="top10nl:_Top10nlObjectType">
                <sequence>
                    <element name="typeLandgebruik" type="gml:CodeType">
