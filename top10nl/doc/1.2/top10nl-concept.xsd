    <element name="_Top10nlObject" type="top10nl:_Top10nlObjectType" abstract="true"
             substitutionGroup="gml:AbstractFeature">
        <annotation>
            <documentation>De gemeenschappelijke eigenschappen van een TOP10NL object
                NOTE De abstracte klasse _Top10nlObject heeft alle attributen die TOP10NL objecten gemeenschappelijk
                hebben, zoals identificatie
            </documentation>
        </annotation>
    </element>
    <complexType name="_Top10nlObjectType" abstract="true">
        <complexContent>
            <extension base="gml:AbstractFeatureType">
                <sequence>
                    <element name="identificatie">
                        <annotation>
                            <documentation>Unieke identificatie van het object binnen het domein van NEN 3610.
                                -- Source --
                                NEN 3610:2011
                            </documentation>
                        </annotation>
                        <complexType>
                            <sequence>
                                <element ref="brt:NEN3610ID"/>
                            </sequence>
                        </complexType>
                    </element>
                    <element name="brontype" type="gml:CodeType">
                        <annotation>
                            <documentation>Aanduiding welk soort bron is gebruikt.</documentation>
                        </annotation>
                    </element>
                    <element name="bronactualiteit" type="date">
                        <annotation>
                            <documentation>Datum van de gebruikte bron.</documentation>
                        </annotation>
                    </element>
                    <element name="bronbeschrijving" type="string">
                        <annotation>
                            <documentation>Beschrijving van de gebruikte bron.</documentation>
                        </annotation>
                    </element>
                    <element name="bronnauwkeurigheid" type="double">
                        <annotation>
                            <documentation>Nauwkeurigheid van de gebruikte bron in meters.</documentation>
                        </annotation>
                    </element>
                    <element name="objectBeginTijd" type="date">
                        <annotation>
                            <documentation>Object begintijd: tijdstip waarop het object in de registratie is ontstaan
                                NOTE Dit tijdstip moet altijd gelijk zijn aan het tijdstipregistratie van het eerste
                                voorkomen van het object.
                                Er zijn verschillende datatypes voor de representatie van een tijdstip: Afhankelijk van
                                de gebruikerswensen m.b.t. granulariteit kan o.a. worden gekozen uit types TM_Instant,
                                Date of DateTime.
                                -- Source --
                                NEN 3610:2011
                            </documentation>
                        </annotation>
                    </element>
                    <element name="objectEindTijd" type="date" minOccurs="0">
                        <annotation>
                            <documentation>Object eindtijd: Tiijdstip waarop het object in de registratie niet meer
                                geldig is
                                NOTE Er zijn verschillende datatypes voor de representatie van een tijdstip: Afhankelijk
                                van de gebruikerswensen m.b.t. granulariteit kan o.a. worden gekozen uit types
                                TM_Instant, Date of DateTime.
                                -- Source --
                                NEN 3610:2011
                            </documentation>
                        </annotation>
                    </element>
                    <element name="tijdstipRegistratie" type="date">
                        <annotation>
                            <documentation>Tijdstip waarop de versie van het object is opgenomen in de registratie
                            </documentation>
                        </annotation>
                    </element>
                    <element name="eindRegistratie" type="date" minOccurs="0"/>
                    <element name="tdnCode" type="integer" minOccurs="0">
                        <annotation>
                            <documentation>Code die het object in TOP10vector had.</documentation>
                        </annotation>
                    </element>
                    <element name="visualisatieCode" type="integer" minOccurs="0">
                        <annotation>
                            <documentation>Code op basis waarvan een gebruiker het object kan visualiseren.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="mutatieType" type="gml:CodeType" minOccurs="0">
                        <annotation>
                            <documentation>Het soort mutatie dat een object als laatste ondergaan heeft.</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="_Top10nlObjectPropertyType">
        <sequence minOccurs="0">
            <element ref="top10nl:_Top10nlObject"/>
        </sequence>
        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
</schema>
