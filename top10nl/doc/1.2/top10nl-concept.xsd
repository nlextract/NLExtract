        <attributeGroup ref="gml:AssociationAttributeGroup"/>
        <attributeGroup ref="gml:OwnershipAttributeGroup"/>
    </complexType>
    <element name="Wegdeel" type="top10nl:WegdeelType" substitutionGroup="top10nl:_Top10nlObject">
        <annotation>
            <documentation>Kleinste functioneel onafhankelijk stukje weg met gelijkblijvende, homogene eigenschappen en
                relaties voor wegverkeer en vliegverkeer te land
            </documentation>
        </annotation>
    </element>
    <complexType name="WegdeelType">
        <complexContent>
            <extension base="top10nl:_Top10nlObjectType">
                <sequence>
                    <element name="typeInfrastructuur" type="gml:CodeType">
                        <annotation>
                            <documentation>Aanduiding of het object een verbinding, een kruising of iets anders is.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="typeWeg" type="gml:CodeType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Het soort weg</documentation>
                        </annotation>
                    </element>
                    <element name="hoofdverkeersgebruik" type="gml:CodeType" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Soort verkeer waarvoor een weg bestemd is</documentation>
                        </annotation>
                    </element>
                    <element name="fysiekVoorkomen" type="gml:CodeType" minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Fysiek voorkomen: De plaats waar het object zich bevindt t.o.v. andere
                                constructies.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="verhardingsbreedteklasse" type="gml:CodeType" minOccurs="0">
                        <annotation>
                            <documentation>De breedte van de verharding, ingedeeld in klassen</documentation>
                        </annotation>
                    </element>
                    <element name="gescheidenRijbaan" type="brt:BRTJaNeeWaardeType">
                        <annotation>
                            <documentation>Aanduiding of het object onderdeel uitmaakt van een weg met gescheiden
                                rijbanen
                            </documentation>
                        </annotation>
                    </element>
                    <element name="verhardingstype" type="gml:CodeType">
                        <annotation>
                            <documentation>Het type verharding van een wegdeel</documentation>
                        </annotation>
                    </element>
                    <element name="aantalRijstroken" type="integer" minOccurs="0">
                        <annotation>
                            <documentation>Het aantal onderverdelingen van een wegdeel d.m.v. strepen, verschil in
                                verharding of kleur.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="hoogteniveau" type="integer">
                        <annotation>
                            <documentation>De relatieve hoogteligging van het geo-object ten opzichte van een ander
                                geo-object.
                                --Description--
                                Op basis van het hoogteniveau kan worden bepaald op welke wijze geo-objecten elkaar
                                kruisen, waarbij het hoogste object op niveau 0 ligt.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="status" type="gml:CodeType">
                        <annotation>
                            <documentation>De gebruiksstatus van het object.</documentation>
                        </annotation>
                    </element>
                    <element name="naam" type="string" minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>De naam van het wegdeel</documentation>
                        </annotation>
                    </element>
                    <element name="isBAGnaam" type="brt:BRTJaNeeWaardeType" minOccurs="0">
                        <annotation>
                            <documentation>Aanduiding of de naam afkomstig is uit de BAG
                                n
                            </documentation>
                        </annotation>
                    </element>
                    <element name="aWegnummer" type="string" minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Nummering van de rijkswegen (autosnelwegen) behorende tot het nationale
                                wegenstelsel
                            </documentation>
                        </annotation>
                    </element>
                    <element name="nWegnummer" type="string" minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Nummering van de provinciale wegen behorende tot het nationale wegenstelsel
                                en nummering van rijkswegen die nog niet de status van autosnelweg hebben.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="eWegnummer" type="string" minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Nummering van de rijkswegen, die een Europese aansluiting op buitenlandse
                                doorgaande wegen vormen
                            </documentation>
                        </annotation>
                    </element>
                    <element name="sWegnummer" type="string" minOccurs="0" maxOccurs="unbounded">
                        <annotation>
                            <documentation>Nummering van lokale doorgaande wegen binnen / rond de gemeente Amsterdaml.
                            </documentation>
                        </annotation>
                    </element>
                    <element name="afritnummer" type="string" minOccurs="0">
                        <annotation>
                            <documentation>Afritnummering op autosnelweg, vastgesteld door Rijkswaterstaat
                            </documentation>
                        </annotation>
                    </element>
                    <element name="afritnaam" type="string" minOccurs="0">
                        <annotation>
                            <documentation>Afritbenaming op autosnelweg, vastgesteld door Rijkswaterstaat
                            </documentation>
                        </annotation>
                    </element>
                    <element name="knooppuntnaam" type="string" minOccurs="0">
                        <annotation>
                            <documentation>Naam van een knooppunt</documentation>
                        </annotation>
                    </element>
                    <element name="brugnaam" type="string" minOccurs="0">
                        <annotation>
                            <documentation>De naam van een brug.</documentation>
                        </annotation>
                    </element>
                    <element name="tunnelnaam" type="string" minOccurs="0">
                        <annotation>
                            <documentation>Naam van de tunnel</documentation>
                        </annotation>
                    </element>
                    <element name="hartGeometrie" type="brt:BRTLijnOfPuntPropertyType" minOccurs="0">
